        <activity
            android:name=".DhakaActivity"
            android:label="@string/title_activity_dhaka" >
        </activity>
        <activity
            android:name=".KhulnaActivity"
            android:label="@string/title_activity_khulna" >
        </activity>
        <activity
            android:name=".RajshahiActivity"
            android:label="@string/title_activity_rajshahi" >
        </activity>
        <activity
            android:name=".RangpurActivity"
            android:label="@string/title_activity_rangpur" >
        </activity>
        <activity
            android:name=".SylhetActivity"
            android:label="@string/title_activity_sylhet" >
